#include <iostream>
#include<bits/stdc++.h>
using namespace std;
int main()
{
  int year;
  cout<<"Enter the year: ";
  cin>>year;
  if(year % 4 == 0)
  {
    if(year % 100 == 0)
     {
       if(year % 400 == 0)
       cout<<"Leap year";
       else
       cout<<"Not Leap year";
     }
     else 
     cout<<"Leap year";
  }
  else
  cout<<"Not Leap year";

  return 0;
}
